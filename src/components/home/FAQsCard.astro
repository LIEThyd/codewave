---
interface Props {
	p1: string;
	p2: string;

}

const { p1 , p2 } = Astro.props;
---

<div class="space-y-5" >
    <p>{p1}</p>
    <p class="text-white" >{p2}</p>
</div>


<style is:global > 
p {
    text-wrap: balance;
}
body , h1,h2,h3,h4,h5,h6,p{
    font-family: 'Source Code Pro', monospace;
}

</style>